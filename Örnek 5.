#include <stdio.h>

int main()
{
    int a = 10;
    char yazi[8];
    int b = 20;

    printf("a: %d ve b: %d\n\n", a, b);
    printf("programi 8 veya daha fazla karakter girerek test edin\n");
    printf("yazi girin: ");
    gets(yazi);

    printf("girilen kelime: ");
    puts(yazi);
    printf("\na: %d ve b: %d\n", a, b);
return 0;
}
