#include <stdio.h>

int main()
{
    char kelime[20];

    printf("Bir kelime girin: ");
    scanf("%s", kelime);

    int i;
    
    // tamamini karakter karakter yazdirirsak, girilmemis kisim rastgele olur

    for (i=0; i<20; i++)
    {
        printf("%c", kelime[i]);
    }
    printf("\n\n");

return 0;
}
